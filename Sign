import java.io.*;
import java.util.*;

class Sign{
    public static void main(String[] args){
    int n;
    Scanner s = new Scanner(System.in);
    n = s.nextInt();
    if(n>=0)
    {
        if(n==0)
        {
            System.out.println("Zero");
        }
        else
        {
            System.out.println("Positive number");
        }
    }
    else{
        System.out.println("Negative number");
    }
    s.close();
}
}
